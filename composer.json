{
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/florianeckerstorfer/foursquare-php"
        },
        {
            "type": "package",
            "package": {
                "name": "php-kml/php-kml",
                "version": "dev",
                "source": {
                    "url": "http://php-kml.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "trunk/"
                },
                "autoload": {
                    "classmap": ["lib/"]
                }
            }
        }
    ],
    "require": {
        "php":                              ">=5.3.3",
        "florianeckerstorfer/foursquare":   "dev-master",
        "symfony/yaml":                     "2.0.*@dev",
        "symfony/http-foundation":          ">=2.1,<2.3-dev",
        "php-kml/php-kml":                  "dev",
        "silex/silex":                      "1.0.*@dev",
        "twig/twig":                        ">=1.8,<2.0-dev"
    },
    "autoload": {
        "psr-0": {
            "FoursquareToKml": "src/"
        },
        "files": [
            "vendor/php-kml/php-kml/lib/kml.php"
        ]
    }
}
